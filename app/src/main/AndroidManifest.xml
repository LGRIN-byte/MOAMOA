<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.moamoa">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_main"
        android:label="모아모아"
        android:roundIcon="@mipmap/ic_main"
        android:supportsRtl="true"
        android:theme="@style/Theme.MOAMOA">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="LoadingActivity"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"></activity>
        //마이페이지 내 정보 이동
        <activity android:name=".ui.mypage.EditMyinfo"></activity>

    </application>
    <uses-permission android:name="android.permission.INTERACT"></uses-permission>
</manifest>